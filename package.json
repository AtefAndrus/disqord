{
  "name": "disqord",
  "version": "0.1.0",
  "description": "Discord bot that connects to OpenRouter for LLM conversations",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "test": "bun typecheck && bun test",
    "typecheck": "tsc --noEmit",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "bun biome check .",
    "format": "bun biome format --write ."
  },
  "engines": {
    "bun": ">=1.3"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "bun-types": "^1.3.4",
    "typescript": "^5.6.3",
    "@biomejs/biome": "2.3.8"
  }
}
